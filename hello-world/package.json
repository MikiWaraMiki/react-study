    "typesync": "^0.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write --loglevel=warn",
      "eslint --fix"
    ],
    "src/**/*.{gql,graphql,json}": [
      "prettier --write loglevel=warn"
    ]
  }
